  },

	"Figure with Image and Caption": {
    "prefix": "addImg",
    "body": [
      "<figure class=\"figure-sniped-567\">",
      "  <img class=\"img-sniped-567\" src=\"${1:imagen.jpg}\" alt=\"${2:Descripción de la imagen}\">",
      "  <figcaption class=\"figcaption-sniped-567\">${3:Pie de imagen descriptivo.}</figcaption>",
      "</figure>"
