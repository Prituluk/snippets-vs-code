  },

	"Standard Article": {
    "prefix": "addArticleStandar",
    "body": [
      "<article class=\"article-sniped-789\">",
      "  <header class=\"article-header-789\">",
      "    <h1 class=\"article-title-789\">${1:Título del Artículo}</h1>",
