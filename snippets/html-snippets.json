	"Standard Navigation Bar": {
    "prefix": "addNavBarStandar",
    "body": [
      "<nav class=\"navbar-sniped-123\">",
      "  <ul class=\"nav-list-123\">",
      "    <li class=\"nav-item-123\"><a class=\"nav-link-123\" href=\"${1:#}\">${2:Inicio}</a></li>",
      "    <li class=\"nav-item-123\"><a class=\"nav-link-123\" href=\"${3:#}\">${4:Acerca}</a></li>",
      "    <li class=\"nav-item-123\"><a class=\"nav-link-123\" href=\"${5:#}\">${6:Contacto}</a></li>",
      "  </ul>",
