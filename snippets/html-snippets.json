	"Standard Card with Image": {
    "prefix": "addCardStandar",
    "body": [
      "<article class=\"card-sniped-789\">",
      "  <figure class=\"card-figure-789\">",
      "    <img class=\"card-img-789\" src=\"${1:imagen.jpg}\" alt=\"${2:Descripción}\">",
      "  </figure>",
      "  <h2 class=\"card-title-789\">${3:Título de la Card}</h2>",
      "  <p class=\"card-description-789\">${4:Descripción breve del contenido.}</p>",
      "  <a class=\"card-link-789\" href=\"${5:#}\">${6:Ver más}</a>",
      "</article>"
    ],
